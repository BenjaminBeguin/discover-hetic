
	<div>Title : <%= @post.title %></div>
	<div>Content : <%= @post.content %></div>
	<div>url : <%= @post.url %></div>
	<div>author : <%
		if @post.user
			concat @post.user.name
		else
			concat "user deleted"
		end
	 %>
	 	
	</div>
	<div>category : <%= @post.category.label %></div>
	<%	if @post.is_my_post? %>
	<%= link_to "Edit", users_posts_id_path(:id => @post.id), method: :get  %> <br><br>


	<% end %>
	<br><br>